{
    "compilerOptions": {
      "target": "es2020",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "paths": {
        "@backend/*": ["backend/src/*"],
        "@frontend/*": ["frontend/src/*"]
      }
    }
  }